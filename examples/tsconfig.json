{
    "compilerOptions": {
        "strict": true,
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true,
        "noEmitOnError": true,
        "noErrorTruncation": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "target": "ES5",
        "lib": ["es2015", "es2016", "es2017", "esnext.array", "dom"],
        "jsx": "react"
    },
    "include": ["*.ts", "*.tsx"]
}
